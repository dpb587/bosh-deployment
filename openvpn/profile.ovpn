---
profile: |
  client
  dev tun
  proto tcp
  remote ((internal_ip)) 1194
  comp-lzo
  resolv-retry infinite
  nobind
  persist-key
  persist-tun
  mute-replay-warnings
  remote-cert-tls server
  verb 3
  mute 20
  tls-client
  <ca>
  ((openvpn_ca.certificate))
  </ca>
  <cert>
  ((openvpn_client.certificate))
  </cert>
  <key>
  ((openvpn_client.private_key))
  </key>
